{
  "name": "Boggle Solver",
  "version": "1.2.0",
  "permissions": [
    "http://*/*", "https://*/*"
  ],
  "web_accessible_resources": [
    "src/json/data.json"
  ],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["vendor/js/jquery.js", "src/js/trie.js", "src/js/traverser.js", "src/js/content_script.js"]
    }
  ],
  "manifest_version": 2
}
